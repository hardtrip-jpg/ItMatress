config_version=5

[application]

config/name="ItMatress"
run/main_scene="uid://8ihndydi4asp"
config/features=PackedStringArray("4.4")
config/icon="res://icon.svg"

[autoload]

Console="*res://addons/console/console.gd"
SignalManager="*res://globals/scripts/signal_manager.gd"
Global="*res://globals/scripts/global.gd"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"

[debug]

gdscript/warnings/untyped_declaration=2

[display]

window/size/viewport_width=320
window/size/viewport_height=240
window/size/always_on_top=true
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/console/plugin.cfg", "res://addons/phantom_camera/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://globals/": "green",
"res://level_components/": "orange",
"res://levels/": "red",
"res://players/": "purple",
"res://ui/": "blue"
}

